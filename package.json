{
	"name": "cloudflare-duckdb",
	"version": "0.1.0",
	"scripts": {
		"build": "rm -rf dist/ && tsc",
		"build:docker": "docker buildx build --platform linux/amd64 -t tobilg/cloudflare-duckdb .",
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"dev:docker": "docker run --rm -it -p 8080:3000 tobilg/cloudflare-duckdb",
		"start": "wrangler dev",
		"iceberg:prepare": "scripts/setup_marimo.sh",
		"iceberg:create": "cd iceberg-data && marimo edit create-iceberg-data.py",
		"cf-typegen": "wrangler types"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250513.0",
		"@types/node": "^22.13.10",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"wrangler": "^4.15.0"
	},
	"dependencies": {
		"@duckdb/node-api": "^1.2.2-alpha.18",
		"@hono/node-server": "^1.14.1",
		"hono": "^4.7.9"
	}
}
